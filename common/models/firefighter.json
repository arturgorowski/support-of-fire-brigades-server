{
  "name": "firefighter",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "mongodb": {
      "allowExtendedOperators": true
    }
  },
  "ownerRelations": true,
  "hidden": [],
  "properties": {
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "function": {
      "type": "string"
    },
    "age": {
      "type": "string"
    },
    "location": {
      "type": "array"
    }
  },
  "validations": [],
  "relations": {
    "roles": {
      "type": "hasMany",
      "model": "Role",
      "foreignKey": "principalId",
      "through": "RoleMapping"
    },
    "accessTokens": {
      "type": "hasMany",
      "model": "AccessToken",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "prezesOSP",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
